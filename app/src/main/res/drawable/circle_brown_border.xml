<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Background color of the circle -->
    <solid android:color="@color/white" />

    <!-- Size of the circle -->
    <size
        android:width="100dp"
        android:height="100dp" />

    <!-- Brown border -->
    <stroke
        android:width="2dp"
        android:color="@color/brown" />

</shape>

